# Block Persist
/dev/block/bootdevice/by-name/persist		u:object_r:persist_block_device:s0
/dev/block/mmcblk0p26				u:object_r:persist_block_device:s0

# Block Userdata
/dev/block/bootdevice/by-name/userdata		u:object_r:userdata_block_device:s0

# Data files
/data/misc/netmgr/log\.txt			u:object_r:netmgrd_data_file:s0

# Ir
/dev/lirc[0-9]*					u:object_r:lirc_device:s0

# Init
/(vendor|system/vendor)/bin/init\.qcom\.graphics\.sh		u:object_r:init_graphics_exec:s0

